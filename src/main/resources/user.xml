<?xml version='1.0' encoding='utf-8'?>

<hibernate-mapping>
    <class name="ru.af.entity.User" table="user">
        <id name="id" type="int">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
    </class>
</hibernate-mapping>
